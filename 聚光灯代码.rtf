            // \'b8\'fc\'d0\'c2\'b7\'c5\'b4\'f3\'be\'b5\'ce\'bb\'d6\'c3\'ba\'cd\'c4\'da\'c8\'dd\par
            function updateMagnifier(x, y) \{\par
                if (!magnifierEnabled) return;\par
                \par
                // \'b8\'fc\'d0\'c2\'b7\'c5\'b4\'f3\'be\'b5\'ce\'bb\'d6\'c3\par
                magnifier.style.left = x + 'px';\par
                magnifier.style.top = y + 'px';\par
                \par
                // \'b8\'fc\'d0\'c2\'b7\'c5\'b4\'f3\'be\'b5\'c4\'da\'c8\'dd\par
                updateMagnifierContent(x, y);\par
                \par
                // \'b8\'fc\'d0\'c2\'d4\'a4\'c0\'c0\'b4\'b0\'bf\'da\'c4\'da\'c8\'dd\par
                updatePreviewContent(x, y);\par
            \}\par
            \par
            // \'b8\'fc\'d0\'c2\'b7\'c5\'b4\'f3\'be\'b5\'c4\'da\'c8\'dd\par
            function updateMagnifierContent(x, y) \{\par
                // \'bb\'f1\'c8\'a1\'b7\'c5\'b4\'f3\'be\'b5\'b3\'df\'b4\'e7\'ba\'cd\'cb\'f5\'b7\'c5\'b1\'b6\'ca\'fd\par
                const magnifierSize = parseInt(magnifierSizeSlider.value);\par
                const zoomLevel = parseInt(magnifierZoomSlider.value) / 10;\par
                \par
                // \'bc\'c6\'cb\'e3\'b7\'c5\'b4\'f3\'be\'b5\'b2\'a3\'c1\'a7\'b5\'c4\'c6\'ab\'d2\'c6\'c1\'bf\par
                const offsetX = (x - magnifierSize / 2) * (1 - 1/zoomLevel);\par
                const offsetY = (y - magnifierSize / 2) * (1 - 1/zoomLevel);\par
                \par
                // \'c9\'e8\'d6\'c3\'b7\'c5\'b4\'f3\'be\'b5\'b2\'a3\'c1\'a7\'b5\'c4\'ce\'bb\'d6\'c3\'ba\'cd\'cb\'f5\'b7\'c5\par
                magnifierGlass.style.transform = `scale($\{zoomLevel\}) translate($\{offsetX/zoomLevel\}px, $\{offsetY/zoomLevel\}px)`;\par
            \}\par
            \par
            // \'b8\'fc\'d0\'c2\'d4\'a4\'c0\'c0\'b4\'b0\'bf\'da\'c4\'da\'c8\'dd\par
            function updatePreviewContent(x, y) \{\par
                // \'bb\'f1\'c8\'a1\'b7\'c5\'b4\'f3\'be\'b5\'b3\'df\'b4\'e7\'ba\'cd\'cb\'f5\'b7\'c5\'b1\'b6\'ca\'fd\par
                const magnifierSize = parseInt(magnifierSizeSlider.value);\par
                const zoomLevel = parseInt(magnifierZoomSlider.value) / 10;\par
                \par
                // \'bc\'c6\'cb\'e3\'d4\'a4\'c0\'c0\'b4\'b0\'bf\'da\'d6\'d0\'b5\'c4\'c6\'ab\'d2\'c6\'c1\'bf\par
                const previewSize = 120; // \'d4\'a4\'c0\'c0\'b4\'b0\'bf\'da\'b4\'f3\'d0\'a1\par
                const offsetX = (x - magnifierSize / 2) * (1 - 1/zoomLevel);\par
                const offsetY = (y - magnifierSize / 2) * (1 - 1/zoomLevel);\par
                \par
                // \'b4\'b4\'bd\'a8\'c1\'d9\'ca\'b1\'d4\'aa\'cb\'d8\'c0\'b4\'cf\'d4\'ca\'be\'b7\'c5\'b4\'f3\'c4\'da\'c8\'dd\par
                const previewContent = document.createElement('div');\par
                previewContent.className = 'magnifier-preview-content';\par
                previewContent.style.transform = `scale($\{zoomLevel\}) translate($\{offsetX/zoomLevel\}px, $\{offsetY/zoomLevel\}px)`;\par
                previewContent.style.backgroundImage = `url($\{getSlideBackground()\})`;\par
                previewContent.style.backgroundSize = `$\{presentationArea.offsetWidth\}px $\{presentationArea.offsetHeight\}px`;\par
                previewContent.style.backgroundPosition = `-$\{x - magnifierSize/2\}px -$\{y - magnifierSize/2\}px`;\par
                \par
                // \'c7\'e5\'bf\'d5\'d4\'a4\'c0\'c0\'b4\'b0\'bf\'da\'b2\'a2\'cc\'ed\'bc\'d3\'d0\'c2\'c4\'da\'c8\'dd\par
                magnifierPreview.innerHTML = '<div class="magnifier-preview-label">\'b7\'c5\'b4\'f3\'d4\'a4\'c0\'c0</div>';\par
                magnifierPreview.appendChild(previewContent);\par
            \}\par
            \par
            // \'bb\'f1\'c8\'a1\'bb\'c3\'b5\'c6\'c6\'ac\'b1\'b3\'be\'b0\'a3\'a8\'bc\'f2\'bb\'af\'b0\'e6\'b1\'be\'a3\'ac\'ca\'b5\'bc\'ca\'d3\'a6\'d3\'c3\'d6\'d0\'bf\'c9\'c4\'dc\'d0\'e8\'d2\'aa\'b8\'fc\'b8\'b4\'d4\'d3\'b5\'c4\'ca\'b5\'cf\'d6\'a3\'a9\par
            function getSlideBackground() \{\par
                // \'d5\'e2\'c0\'ef\'b7\'b5\'bb\'d8\'d2\'bb\'b8\'f6\'d1\'d5\'c9\'ab\'d7\'f7\'ce\'aa\'b1\'b3\'be\'b0\'a3\'ac\'ca\'b5\'bc\'ca\'d3\'a6\'d3\'c3\'d6\'d0\'bf\'c9\'c4\'dc\'d0\'e8\'d2\'aa\'b2\'b6\'bb\'f1\'c6\'c1\'c4\'bb\'c4\'da\'c8\'dd\par
                return 'data:image/svg+xml;base64,';\par
            \}\par
            \par
            // \'b8\'fc\'d0\'c2\'be\'db\'b9\'e2\'b5\'c6\'ce\'bb\'d6\'c3\par
            function updateSpotlightPosition(x, y) \{\par
                if (!spotlightEnabled) return;\par
                \par
                spotlight.style.left = x + 'px';\par
                spotlight.style.top = y + 'px';\par
                \par
                // \'c8\'e7\'b9\'fb\'bc\'a4\'b9\'e2\'c4\'a3\'ca\'bd\'bf\'aa\'c6\'f4\'a3\'ac\'b8\'fc\'d0\'c2\'bc\'a4\'b9\'e2\'d6\'b8\'d5\'eb\'ce\'bb\'d6\'c3\par
                if (laserMode) \{\par
                    updateLaserPointer(x, y);\par
                \}\par
            \}\par
            \par
            // \'b8\'fc\'d0\'c2\'bc\'a4\'b9\'e2\'d6\'b8\'d5\'eb\par
            function updateLaserPointer(x, y) \{\par
                if (!laserEnabled) return;\par
                \par
                laserPointer.style.left = x + 'px';\par
                laserPointer.style.top = y + 'px';\par
                laserPointer.style.opacity = '1';\par
                \par
                // \'c7\'e5\'b3\'fd\'d6\'ae\'c7\'b0\'b5\'c4\'b5\'ad\'b3\'f6\'b6\'a8\'ca\'b1\'c6\'f7\par
                if (laserTimeout) \{\par
                    clearTimeout(laserTimeout);\par
                \}\par
                \par
                // \'c9\'e8\'d6\'c3\'d0\'c2\'b5\'c4\'b5\'ad\'b3\'f6\'b6\'a8\'ca\'b1\'c6\'f7\par
                laserTimeout = setTimeout(() => \{\par
                    laserPointer.style.opacity = '0';\par
                    if (laserLine) \{\par
                        laserLine.style.opacity = '0';\par
                    \}\par
                \}, 500);\par
                \par
                // \'b8\'fc\'d0\'c2\'bc\'a4\'b9\'e2\'cf\'df\par
                if (lastMouseX && lastMouseY) \{\par
                    const dx = x - lastMouseX;\par
                    const dy = y - lastMouseY;\par
                    const distance = Math.sqrt(dx * dx + dy * dy);\par
                    const angle = Math.atan2(dy, dx) * 180 / Math.PI;\par
                    \par
                    if (distance > 5) \{ // \'b1\'dc\'c3\'e2\'b6\'b6\'b6\'af\par
                        const line = createLaserLine();\par
                        line.style.width = distance + 'px';\par
                        line.style.left = lastMouseX + 'px';\par
                        line.style.top = lastMouseY + 'px';\par
                        line.style.transform = `rotate($\{angle\}deg)`;\par
                        line.style.opacity = '0.6';\par
                        \par
                        // \'b5\'ad\'b3\'f6\'bc\'a4\'b9\'e2\'cf\'df\par
                        setTimeout(() => \{\par
                            if (line) line.style.opacity = '0';\par
                        \}, 300);\par
                    \}\par
                \}\par
                \par
                lastMouseX = x;\par
                lastMouseY = y;\par
            \}\par
            \par
            // \'ca\'f3\'b1\'ea\'d2\'c6\'b6\'af\'ca\'c2\'bc\'fe\par
            presentationArea.addEventListener('mousemove', (e) => \{\par
                updateMagnifier(e.clientX, e.clientY);\par
                updateSpotlightPosition(e.clientX, e.clientY);\par
            \});\par
            \par
            // \'ca\'f3\'b1\'ea\'b5\'e3\'bb\'f7\'ca\'c2\'bc\'fe - \'cf\'d4\'ca\'be\'bc\'a4\'b9\'e2\'d6\'b8\'d5\'eb\par
            presentationArea.addEventListener('mousedown', (e) => \{\par
                if (e.button === 0) \{ // \'d7\'f3\'bc\'fc\'b5\'e3\'bb\'f7\par
                    updateLaserPointer(e.clientX, e.clientY);\par
                \}\par
            \});\par
            \par
            // \'bf\'d8\'d6\'c6\'b7\'c5\'b4\'f3\'be\'b5\'b4\'f3\'d0\'a1\par
            magnifierSizeSlider.addEventListener('input', function() \{\par
                const size = this.value + 'px';\par
                magnifier.style.width = size;\par
                magnifier.style.height = size;\par
                magnifierSizeValue.textContent = size;\par
            \});\par
            \par
            // \'bf\'d8\'d6\'c6\'b7\'c5\'b4\'f3\'be\'b5\'cb\'f5\'b7\'c5\'b1\'b6\'ca\'fd\par
            magnifierZoomSlider.addEventListener('input', function() \{\par
                const zoomLevel = (this.value / 10).toFixed(1);\par
                magnifierZoomValue.textContent = zoomLevel + 'x';\par
            \});\par
            \par
            // \'bf\'d8\'d6\'c6\'be\'db\'b9\'e2\'b5\'c6\'b4\'f3\'d0\'a1\par
            spotlightSizeSlider.addEventListener('input', function() \{\par
                const size = this.value + 'px';\par
                spotlight.style.width = size;\par
                spotlight.style.height = size;\par
                spotlightSizeValue.textContent = size;\par
            \});\par
            \par
            // \'bf\'d8\'d6\'c6\'be\'db\'b9\'e2\'b5\'c6\'c1\'c1\'b6\'c8\par
            spotlightBrightnessSlider.addEventListener('input', function() \{\par
                const brightness = this.value / 100;\par
                spotlight.style.background = `radial-gradient(circle, rgba(255, 255, 255, $\{brightness\}) 0%, rgba(255, 255, 255, 0) 70%)`;\par
                spotlightBrightnessValue.textContent = brightness.toFixed(2);\par
            \});\par
            \par
            // \'c9\'cf\'d2\'bb\'d2\'b3\par
            prevSlideBtn.addEventListener('click', () => \{\par
                if (currentSlide > 0) \{\par
                    currentSlide--;\par
                    updateSlide();\par
                \}\par
            \});\par
            \par
            // \'cf\'c2\'d2\'bb\'d2\'b3\par
            nextSlideBtn.addEventListener('click', () => \{\par
                if (currentSlide < slides.length - 1) \{\par
                    currentSlide++;\par
                    updateSlide();\par
                \}\par
            \});\par
            \par
            // \'c7\'d0\'bb\'bb\'b7\'c5\'b4\'f3\'be\'b5\'bf\'aa\'b9\'d8\par
            toggleMagnifierBtn.addEventListener('click', () => \{\par
                magnifierEnabled = !magnifierEnabled;\par
                magnifier.style.display = magnifierEnabled ? 'block' : 'none';\par
                magnifierStatus.textContent = magnifierEnabled ? '\'bf\'aa\'c6\'f4' : '\'b9\'d8\'b1\'d5';\par
                magnifierPreview.style.display = magnifierEnabled ? 'flex' : 'none';\par
                toggleMagnifierBtn.innerHTML = magnifierEnabled ? \par
                    '<i class="fas fa-search-plus"></i> \'b9\'d8\'b1\'d5\'b7\'c5\'b4\'f3\'be\'b5' : \par
                    '<i class="fas fa-search-plus"></i> \'bf\'aa\'c6\'f4\'b7\'c5\'b4\'f3\'be\'b5';\par
                \par
                // \'b8\'fc\'d0\'c2\'b9\'e2\'b1\'ea\'d1\'f9\'ca\'bd\par
                presentationArea.style.cursor = magnifierEnabled ? 'none' : 'default';\par
            \});\par
            \par
            // \'c7\'d0\'bb\'bb\'be\'db\'b9\'e2\'b5\'c6\'bf\'aa\'b9\'d8\par
            toggleSpotlightBtn.addEventListener('click', () => \{\par
                spotlightEnabled = !spotlightEnabled;\par
                spotlight.style.opacity = spotlightEnabled ? '1' : '0';\par
                spotlightStatus.textContent = spotlightEnabled ? '\'bf\'aa\'c6\'f4' : '\'b9\'d8\'b1\'d5';\par
                toggleSpotlightBtn.innerHTML = spotlightEnabled ? \par
                    '<i class="fas fa-lightbulb"></i> \'b9\'d8\'b1\'d5\'be\'db\'b9\'e2\'b5\'c6' : \par
                    '<i class="fas fa-lightbulb"></i> \'bf\'aa\'c6\'f4\'be\'db\'b9\'e2\'b5\'c6';\par
            \});\par
            \par
            // \'c7\'d0\'bb\'bb\'bc\'a4\'b9\'e2\'bf\'aa\'b9\'d8\par
            toggleLaserBtn.addEventListener('click', () => \{\par
                laserMode = !laserMode;\par
                laserEnabled = laserMode;\par
                laserStatus.textContent = laserMode ? '\'bf\'aa\'c6\'f4' : '\'b9\'d8\'b1\'d5';\par
                toggleLaserBtn.innerHTML = laserMode ? \par
                    '<i class="fas fa-mouse-pointer"></i> \'b9\'d8\'b1\'d5\'bc\'a4\'b9\'e2' : \par
                    '<i class="fas fa-mouse-pointer"></i> \'bf\'aa\'c6\'f4\'bc\'a4\'b9\'e2';\par
                \par
                // \'c8\'e7\'b9\'fb\'b9\'d8\'b1\'d5\'bc\'a4\'b9\'e2\'a3\'ac\'d2\'fe\'b2\'d8\'bc\'a4\'b9\'e2\'d6\'b8\'d5\'eb\par
                if (!laserMode) \{\par
                    laserPointer.style.opacity = '0';\par
                    if (laserLine) \{\par
                        laserLine.style.opacity = '0';\par
                    \}\par
                \}\par
            \});\par
            \par
            // \'d6\'d8\'d6\'c3\'cb\'f9\'d3\'d0\'c9\'e8\'d6\'c3\par
            resetAllBtn.addEventListener('click', resetAllSettings);\par
            \par
            // \'b5\'e7\'d4\'b4\'b0\'b4\'c5\'a5\par
            powerBtn.addEventListener('click', () => \{\par
                isPowerOn = !isPowerOn;\par
                powerBtn.classList.toggle('on', isPowerOn);\par
                \par
                if (isPowerOn) \{\par
                    // \'bf\'aa\'c6\'f4\'cb\'f9\'d3\'d0\'b9\'a6\'c4\'dc\par
                    resetAllSettings();\par
                \} else \{\par
                    // \'b9\'d8\'b1\'d5\'cb\'f9\'d3\'d0\'b9\'a6\'c4\'dc\par
                    magnifierEnabled = false;\par
                    spotlightEnabled = false;\par
                    laserMode = false;\par
                    laserEnabled = false;\par
                    \par
                    magnifier.style.display = 'none';\par
                    spotlight.style.opacity = '0';\par
                    laserPointer.style.opacity = '0';\par
                    magnifierPreview.style.display = 'none';\par
                    \par
                    magnifierStatus.textContent = '\'b9\'d8\'b1\'d5';\par
                    spotlightStatus.textContent = '\'b9\'d8\'b1\'d5';\par
                    laserStatus.textContent = '\'b9\'d8\'b1\'d5';\par
                    \par
                    toggleMagnifierBtn.innerHTML = '<i class="fas fa-search-plus"></i> \'bf\'aa\'c6\'f4\'b7\'c5\'b4\'f3\'be\'b5';\par
                    toggleSpotlightBtn.innerHTML = '<i class="fas fa-lightbulb"></i> \'bf\'aa\'c6\'f4\'be\'db\'b9\'e2\'b5\'c6';\par
                    toggleLaserBtn.innerHTML = '<i class="fas fa-mouse-pointer"></i> \'bf\'aa\'c6\'f4\'bc\'a4\'b9\'e2';\par
                    \par
                    presentationArea.style.cursor = 'default';\par
                \}\par
            \});\par
            \par
            // \'c7\'d0\'bb\'bb\'bf\'d8\'d6\'c6\'c3\'e6\'b0\'e5\'cf\'d4\'ca\'be\par
            togglePanelBtn.addEventListener('click', () => \{\par
                controlPanel.classList.toggle('hidden');\par
                togglePanelBtn.innerHTML = controlPanel.classList.contains('hidden') ? \par
                    '<i class="fas fa-sliders-h"></i>' : \par
                    '<i class="fas fa-times"></i>';\par
            \});\par
            \par
            // \'d6\'d8\'d6\'c3\'cb\'f9\'d3\'d0\'c9\'e8\'d6\'c3\par
            function resetAllSettings() \{\par
                // \'b7\'c5\'b4\'f3\'be\'b5\'c9\'e8\'d6\'c3\par
                magnifierSizeSlider.value = 180;\par
                magnifierZoomSlider.value = 18;\par
                magnifier.style.width = '180px';\par
                magnifier.style.height = '180px';\par
                magnifierSizeValue.textContent = '180px';\par
                magnifierZoomValue.textContent = '1.8x';\par
                \par
                // \'be\'db\'b9\'e2\'b5\'c6\'c9\'e8\'d6\'c3\par
                spotlightSizeSlider.value = 300;\par
                spotlightBrightnessSlider.value = 15;\par
                spotlight.style.width = '300px';\par
                spotlight.style.height = '300px';\par
                spotlight.style.background = 'radial-gradient(circle, rgba(255, 255, 255, 0.15) 0%, rgba(255, 255, 255, 0) 70%)';\par
                spotlightSizeValue.textContent = '300px';\par
                spotlightBrightnessValue.textContent = '0.15';\par
                \par
                // \'bf\'aa\'c6\'f4\'cb\'f9\'d3\'d0\'b9\'a6\'c4\'dc\par
                magnifierEnabled = true;\par
                spotlightEnabled = true;\par
                laserMode = false;\par
                laserEnabled = false;\par
                \par
                magnifier.style.display = 'block';\par
                spotlight.style.opacity = '1';\par
                laserPointer.style.opacity = '0';\par
                magnifierPreview.style.display = 'flex';\par
                \par
                magnifierStatus.textContent = '\'bf\'aa\'c6\'f4';\par
                spotlightStatus.textContent = '\'bf\'aa\'c6\'f4';\par
                laserStatus.textContent = '\'b9\'d8\'b1\'d5';\par
                \par
                toggleMagnifierBtn.innerHTML = '<i class="fas fa-search-plus"></i> \'b9\'d8\'b1\'d5\'b7\'c5\'b4\'f3\'be\'b5';\par
                toggleSpotlightBtn.innerHTML = '<i class="fas fa-lightbulb"></i> \'b9\'d8\'b1\'d5\'be\'db\'b9\'e2\'b5\'c6';\par
                toggleLaserBtn.innerHTML = '<i class="fas fa-mouse-pointer"></i> \'bf\'aa\'c6\'f4\'bc\'a4\'b9\'e2';\par
                \par
                presentationArea.style.cursor = 'none';\par
                \par
                // \'bd\'ab\'be\'db\'b9\'e2\'b5\'c6\'ba\'cd\'b7\'c5\'b4\'f3\'be\'b5\'d2\'c6\'b6\'af\'b5\'bd\'d6\'d0\'d0\'c4\'ce\'bb\'d6\'c3\par
                const centerX = window.innerWidth / 2;\par
                const centerY = window.innerHeight / 2;\par
                updateMagnifier(centerX, centerY);\par
                updateSpotlightPosition(centerX, centerY);\par
            \}\par
            \par
            // \'b8\'fc\'d0\'c2\'bb\'c3\'b5\'c6\'c6\'ac\'cf\'d4\'ca\'be\par
            function updateSlide() \{\par
                // \'d2\'fe\'b2\'d8\'cb\'f9\'d3\'d0\'bb\'c3\'b5\'c6\'c6\'ac\par
                slides.forEach(slide => \{\par
                    slide.classList.remove('active');\par
                \});\par
                \par
                // \'cf\'d4\'ca\'be\'b5\'b1\'c7\'b0\'bb\'c3\'b5\'c6\'c6\'ac\par
                slides[currentSlide].classList.add('active');\par
                \par
                // \'b8\'fc\'d0\'c2\'d2\'b3\'c2\'eb\'cf\'d4\'ca\'be\par
                slideNumberElement.textContent = currentSlide + 1;\par
                \par
                // \'cc\'ed\'bc\'d3\'bb\'c3\'b5\'c6\'c6\'ac\'c7\'d0\'bb\'bb\'b6\'af\'bb\'ad\par
                const slideContainer = document.querySelector('.slide-container');\par
                slideContainer.style.transform = `translate(-50%, -50%) scale(0.98)`;\par
                setTimeout(() => \{\par
                    slideContainer.style.transform = `translate(-50%, -50%) scale(1)`;\par
                \}, 50);\par
            \}\par
            \par
            // \'bc\'fc\'c5\'cc\'bf\'d8\'d6\'c6\par
            document.addEventListener('keydown', (e) => \{\par
                switch(e.key) \{\par
                    case 'ArrowLeft':\par
                        if (currentSlide > 0) \{\par
                            currentSlide--;\par
                            updateSlide();\par
                        \}\par
                        break;\par
                    case 'ArrowRight':\par
                        if (currentSlide < slides.length - 1) \{\par
                            currentSlide++;\par
                            updateSlide();\par
                        \}\par
                        break;\par
                    case 'm':\par
                    case 'M':\par
                        e.preventDefault();\par
                        toggleMagnifierBtn.click();\par
                        break;\par
                    case ' ':\par
                        e.preventDefault();\par
                        toggleSpotlightBtn.click();\par
                        break;\par
                    case 'l':\par
                    case 'L':\par
                        toggleLaserBtn.click();\par
                        break;\par
                    case 'Escape':\par
                        magnifierEnabled = false;\par
                        spotlightEnabled = false;\par
                        laserMode = false;\par
                        laserEnabled = false;\par
                        \par
                        magnifier.style.display = 'none';\par
                        spotlight.style.opacity = '0';\par
                        laserPointer.style.opacity = '0';\par
                        magnifierPreview.style.display = 'none';\par
                        \par
                        magnifierStatus.textContent = '\'b9\'d8\'b1\'d5';\par
                        spotlightStatus.textContent = '\'b9\'d8\'b1\'d5';\par
                        laserStatus.textContent = '\'b9\'d8\'b1\'d5';\par
                        \par
                        toggleMagnifierBtn.innerHTML = '<i class="fas fa-search-plus"></i> \'bf\'aa\'c6\'f4\'b7\'c5\'b4\'f3\'be\'b5';\par
                        toggleSpotlightBtn.innerHTML = '<i class="fas fa-lightbulb"></i> \'bf\'aa\'c6\'f4\'be\'db\'b9\'e2\'b5\'c6';\par
                        toggleLaserBtn.innerHTML = '<i class="fas fa-mouse-pointer"></i> \'bf\'aa\'c6\'f4\'bc\'a4\'b9\'e2';\par
                        \par
                        presentationArea.style.cursor = 'default';\par
                        break;\par
                    case 'r':\par
                    case 'R':\par
                        e.preventDefault();\par
                        resetAllSettings();\par
                        break;\par
                \}\par
            \});\par
            \par
            // \'b4\'a5\'c3\'fe\'c6\'c1\'d6\'a7\'b3\'d6\par
            presentationArea.addEventListener('touchmove', (e) => \{\par
                e.preventDefault();\par
                const touch = e.touches[0];\par
                updateMagnifier(touch.clientX, touch.clientY);\par
                updateSpotlightPosition(touch.clientX, touch.clientY);\par
            \});\par
            \par
            presentationArea.addEventListener('touchstart', (e) => \{\par
                e.preventDefault();\par
                const touch = e.touches[0];\par
                updateLaserPointer(touch.clientX, touch.clientY);\par
            \});\par
            \par
            // \'b4\'b0\'bf\'da\'b4\'f3\'d0\'a1\'b1\'e4\'bb\'af\'ca\'b1\'d6\'d8\'d0\'c2\'b6\'a8\'ce\'bb\par
            window.addEventListener('resize', () => \{\par
                // \'d6\'d8\'d0\'c2\'b3\'f5\'ca\'bc\'bb\'afCanvas\par
                initCanvas();\par
                \par
                // \'bd\'ab\'b7\'c5\'b4\'f3\'be\'b5\'ba\'cd\'be\'db\'b9\'e2\'b5\'c6\'d6\'c3\'d3\'da\'d6\'d0\'d0\'c4\'ce\'bb\'d6\'c3\par
                const centerX = window.innerWidth / 2;\par
                const centerY = window.innerHeight / 2;\par
                updateMagnifier(centerX, centerY);\par
                updateSpotlightPosition(centerX, centerY);\par
